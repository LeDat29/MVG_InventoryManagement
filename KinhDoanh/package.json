{
  "name": "kho-mvg-management",
  "version": "1.0.0",
  "description": "Hệ thống quản lý hỗ trợ kinh doanh các dự án kho xưởng - KHO MVG",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:test": "cross-env NODE_ENV=test node server.js",
    "start:client": "cross-env PORT=3000 BROWSER=none npm start --prefix client",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:e2e": "start-server-and-test \"npm run start:test\" http://localhost:5001 \"npm run start:client\" http://localhost:3000 \"npm run test:e2e:run\"",
    "test:e2e:run": "jest --config jest-e2e.config.js --runInBand",
    "playwright:install": "npx playwright install --with-deps",
    "test:e2e:playwright:run": "npx playwright test tests/playwright --headed --reporter=list",
    "test:e2e:playwright": "start-server-and-test \"npm run start:test\" http://localhost:5001 \"npm run start:client\" http://localhost:3000 \"npm run test:e2e:playwright:run\"",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "warehouse",
    "management",
    "nodejs",
    "react",
    "pwa"
  ],
  "author": "KHO MVG Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^2.3.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.15.0",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.1",
    "pdfkit": "^0.13.0",
    "sharp": "^0.32.1",
    "socket.io": "^4.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^8.3.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@types/jest": "^29.5.14",
    "axios": "^1.13.2",
    "babel-jest": "^30.2.0",
    "cross-env": "^7.0.3",
    "@playwright/test": "^1.40.0",
    "playwright": "^1.40.0",
    "eslint": "^9.39.1",
    "expect-puppeteer": "^11.0.0",
    "jest": "^29.7.0",
    "jest-puppeteer": "^11.0.0",
    "jsdoc": "^4.0.2",
    "nodemon": "^2.0.22",
    "puppeteer": "^24.33.0",
    "start-server-and-test": "^2.1.3",
    "supertest": "^6.3.4"
  }
}
